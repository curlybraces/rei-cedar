/* ---------------------
* Motion
* ------------------- */
$timing-function-ease: cubic-bezier(0.15, 0, 0.15, 0);
$timing-function-ease-out: cubic-bezier(0.32, 0.94, 0.6, 1);
$duration-1-x: 100ms;
$duration-2-x: 200ms;
$duration-3-x: 300ms;
$duration-4-x: 400ms;
$rotation-90: 90;
$duration-5-x: 500ms;
$duration-6-x: 600ms;
$timing-function-linear: cubic-bezier(0, 0, 1, 1);
$rotation-180: 180;
$rotation-360: 360;

/* ---------------------
* Radius
* ------------------- */
$radius-sharp: 0;
$radius-soft: 2px;
$radius-softer: 4px;
$radius-round: 50%;

/* ---------------------
* Prominence
* ------------------- */
$prominence-flat: 0 0 0 0 rgba(26, 26, 26, 0.2);
$prominence-raised: 2px 2px 2px 0 rgba(26, 26, 26, 0.2);
$prominence-elevated: 4px 4px 4px 0 rgba(26, 26, 26, 0.2);
$prominence-floating: 8px 8px 8px 0 rgba(26, 26, 26, 0.2);
$prominence-lifted: 16px 16px 16px 0 rgba(26, 26, 26, 0.2);

/* ---------------------
