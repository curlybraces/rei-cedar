{
  "name": "@rei/cedar",
  "version": "1.0.0-alpha.1",
  "description": "REI Cedar Component Library",
  "homepage": "https://rei.github.io/rei-cedar/",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "REI Software Engineering",
  "main": "dist/cedar.cjs.js",
  "module": "dist/cedar.esm.js",
  "style": "dist/cedar.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "cross-env NODE_ENV=dev rollup -w -c rollup.config.dev.js",
    "build": "npm run clean && npm run build:csr && npm run build:ssr",
    "build:csr": "cross-env NODE_ENV=prod rollup -c rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=prod SSR_ENV=ssr rollup -c rollup.config.js",
    "unit": "cross-env NODE_ENV=test rollup -c rollup.config.test.js && jest --clearCache && jest",
    "unit:watch": "cross-env NODE_ENV=test nyc mocha-webpack --watch --webpack-config build/webpack.test.conf.js src/**/*.spec.js",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.vue src",
    "reference": "backstop reference --configPath=backstop.js",
    "compare": "backstop test --configPath=backstop.js",
    "approve": "backstop approve --configPath=backstop.js",
    "docs": "vue-styleguidist server",
    "build:archive": "node build/component-docs-archive.js",
    "build:transfer": "node build/component-docs-transfer.js",
    "create": "node generator.js",
    "check-version": "node check-version.js",
    "vue:build": "npm run vue:docs && npm run vue:archive && npm run vue:transfer",
    "component-css": "cross-env NODE_ENV=css node build/component-css-build.js"
  },
  "devDependencies": {
