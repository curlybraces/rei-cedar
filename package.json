    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-vue": "^5.0.0",
    "selenium-server": "^3.141.59",
    "semver": "^5.6.0",
    "semver-diff": "^2.1.0",
    "serve": "^11.0.0",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
